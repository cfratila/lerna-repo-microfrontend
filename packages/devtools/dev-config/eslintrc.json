{
  "extends": [
    "prettier",
    "react-app"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "globals": {
    "importScripts": true,
    "cy": true,
    "context": true,
    "shallow": true,
    "window": true,
    "document": true,
    "APP_TOKEN": true,
    "config": true,
    "Auth": true,
    "APP_ROOT": true,
    "logger": true,
    "debug": true,
    "crypto": true
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-const-assign": 1,
    "no-extra-semi": 0,
    "semi": 0,
    "no-empty": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-this-before-super": 1,
    "class-methods-use-this": 0,
    "no-undef": 1,
    "no-unreachable": 1,
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "debug",
        "ignoreRestSiblings": true,
        "argsIgnorePattern": "next"
      }
    ],
    "no-unused-expressions": [
      "error",
      {
        "allowTaggedTemplates": true,
        "allowShortCircuit": true
      }
    ],
    "no-use-before-define": 0,
    "constructor-super": 1,
    "func-names": 0,
    "comma-dangle": 0,
    "prefer-arrow-callback": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "camelcase": 0,
    "no-bitwise": 0,
    "consistent-return": 0,
    "max-len": 0,
    "no-mixed-operators": 0,
    "curly": 0,
    "object-curly-newline": 0,
    "import/no-unresolved": 0,
    "no-restricted-syntax": 0,
    "no-prototype-builtins": 0,
    "no-continue": 0,
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "arrow-parens": 0,
    "react/prop-types": 1,
    "react/display-name": 0,
    "react/no-deprecated": 0,
    "react/require-default-props": 1,
    "react/no-multi-comp": 1
  }
}
